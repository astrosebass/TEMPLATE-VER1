  <POU Name="B_1_1_PRG_Master_Station_1" Id="{74fda7da-e1c3-493f-8c86-5902a8ab2668}" SpecialFunc="None">
    <Declaration><![CDATA[PROGRAM B_1_1_PRG_Master_Station_1

VAR
	eState								: E_Station1_State;
	tonErrorReset						: TON;
	tonStationStopped					: TON;
	ctuErrorCounter						: CTU;
	rGoodStatistics						: R_TRIG;
	rBadStatistics						: R_TRIG;
	rEfficency							: STRING(510);
	
	bPersistentError					: bool;
/////////////////////////////////////////////////////////////////////////////////////I O/////////////////////////////////////////////////////////////////////////////////////	
	bStopped							: BOOL;
	bError								: BOOL;
	bReady								: BOOL;
	bDone								: BOOL;
	bPass								: BOOL;
	bFail								: BOOL;

	
END_VAR

VAR_INPUT	
	
	CMD									:ST_StationCMD;	
	
END_VAR

VAR_OUTPUT	
	
	STATUS								:ST_StationStatus;	
	
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[B_1_2_PRG_Fault_Station_1( aAlarms := stAlarms );
a_ACT_Ctrl_Station_1();
b_ACT_IO();
c_ACT_Statistics_Station_1();]]></ST>
    </Implementation>
    <Action Name="a_ACT_Ctrl_Station_1" Id="{ce5dd8ea-9899-4a9f-bde8-defccc1c38a2}">
      <Implementation>
        <ST><![CDATA[/////////////////////////////////////////////////////////////////////////////////////UTILITIES/////////////////////////////////////////////////////////////////////////////////////
tonErrorReset( IN := (eState = E_Station1_State.RESET_ERROR), PT := T#500MS);
tonStationStopped( IN := bStopped, PT := T#1S);

ctuErrorCounter( CU := eState = E_Station1_State.RESET_ERROR, RESET := ((ctuErrorCounter.CV = 5) AND eState = E_Station1_State.PERSISTENT_ERROR), PV := 5, Q => bPersistentError);



/////////////////////////////////////////////////////////////////////////////////////CONDITIONS/////////////////////////////////////////////////////////////////////////////////////

IF (CMD.bAbort OR bError) AND ( eState			= E_Station1_State.HOMING OR
								eState			= E_Station1_State.READY OR
								eState			= E_Station1_State.MOVEMENT_A OR
								eState			= E_Station1_State.MOVEMENT_B OR
								eState			= E_Station1_State.MOVEMENT_C OR
								eState			= E_Station1_State.READY OR
								eState			= E_Station1_State.DONE) THEN
								
	eState						:= E_Station1_State.STOPPING;
	
END_IF



CASE eState OF
	
	E_Station1_State.ABORTED:
								IF CMD.bEnable AND NOT bError THEN
									eState						:= E_Station1_State.HOMING;
								ELSIF bError THEN
									eState						:= E_Station1_State.ERROR;
								END_IF
	
	E_Station1_State.HOMING:
								IF bReady THEN
									eState						:= E_Station1_State.RESET;
								END_IF
																
	E_Station1_State.RESET:
								stCycleDataStation1.bPass	:= FALSE;
								stCycleDataStation1.bFail	:= FALSE;
								
								eState						:= E_Station1_State.READY;
	E_Station1_State.READY:
								IF CMD.bExecute AND stStation1.Mov1 THEN
									eState						:= E_Station1_State.MOVEMENT_A;
								ELSIF CMD.bExecute AND stStation1.Mov2 THEN
									eState						:= E_Station1_State.MOVEMENT_B;
								ELSIF CMD.bExecute AND stStation1.Mov3 THEN
									eState						:= E_Station1_State.MOVEMENT_C;
								END_IF
	
	E_Station1_State.MOVEMENT_A:
								IF bDone AND stStation1.Mov1 THEN
									eState						:= E_Station1_State.DONE;
								END_IF
	
	E_Station1_State.MOVEMENT_B:
								IF bDone AND stStation1.Mov2 THEN
									eState						:= E_Station1_State.DONE;
								END_IF
	E_Station1_State.MOVEMENT_C:
								IF bDone AND stStation1.Mov3 THEN
									eState						:= E_Station1_State.DONE;
								END_IF
	E_Station1_State.DONE:
								IF  CMD.bReady THEN
									eState						:= E_Station1_State.RESULTS;
								END_IF	
								
	E_Station1_State.RESULTS:
								
								IF bPass THEN
									stCycleDataStation1.bPass	:= TRUE;
								ELSIF bFail THEN
								 	stCycleDataStation1.bFail	:= TRUE;
								END_IF
								
								IF  CMD.bEndCycle THEN
									eState						:= E_Station1_State.RESET;
								END_IF
								
/////////////////////////////////////////////////////////////////////////////////////RESET CMD/////////////////////////////////////////////////////////////////////////////////////			
	
	E_Station1_State.STOPPING: 
								IF tonStationStopped.Q THEN
									IF bError THEN
										eState						:= E_Station1_State.ERROR;
									ELSE eState						:= E_Station1_State.ABORTED;
									END_IF

								END_IF
	
	E_Station1_State.ERROR:
								IF NOT bError THEN
									eState						:= E_Station1_State.ABORTED;
								ELSE eState						:= E_Station1_State.RESET_ERROR;
								END_IF
	
	E_Station1_State.RESET_ERROR:
								IF tonErrorReset.Q AND NOT bPersistentError THEN
									eState						:= E_Station1_State.ERROR;
								ELSIF bPersistentError THEN
									eState						:= E_Station1_State.PERSISTENT_ERROR;
								END_IF
	
	E_Station1_State.PERSISTENT_ERROR:
								IF CMD.bReset THEN
									eState						:= E_Station1_State.ERROR;
								END_IF
	


							
						
								
													
								

ELSE eState := E_Station1_State.ABORTED;
	
END_CASE
/////////////////////////////////////////////////////////////////////////////////////STATUS/////////////////////////////////////////////////////////////////////////////////////

Status.bBusy				:= 	eState			= E_Station1_State.HOMING OR
								eState			= E_Station1_State.MOVEMENT_A OR
								eState			= E_Station1_State.MOVEMENT_B OR
								eState			= E_Station1_State.MOVEMENT_C OR
								eState			= E_Station1_State.STOPPING;
								
Status.bDone				:=	eState			= E_Station1_State.DONE;
											
Status.bError				:= 	(eState			= E_Station1_State.ERROR) OR 
								(eState			= E_Station1_State.RESET_ERROR) OR
								(eState			= E_Station1_State.PERSISTENT_ERROR);
								
Status.bReady				:= 	eState			= E_Station1_State.READY;

Status.bStopped				:= 	eState			= E_Station1_State.ABORTED OR
								eState			= E_Station1_State.READY OR
								eState			= E_Station1_State.ERROR OR
								eState			= E_Station1_State.RESET_ERROR;]]></ST>
      </Implementation>
    </Action>
    <Action Name="b_ACT_IO" Id="{0f51cbd8-7d44-46ba-8ff2-5b85b0a3ce4b}">
      <Implementation>
        <ST><![CDATA[//bStopped								:=;
//bError								:=;
//bReady								:=;
//bDone]]></ST>
